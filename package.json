{
  "name": "@chiballc/nuxt-form-builder",
  "version": "0.0.27",
  "description": "For Builder Nuxt Module",
  "repository": "kgarchie/form-builder",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./dist/types.d.ts",
        "./dist/runtime/types.d.ts"
      ],
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.3",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.7.1",
    "@nuxt/schema": "^3.12.1",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.14.2",
    "changelogen": "^0.5.5",
    "eslint": "^9.4.0",
    "nuxt": "^3.12.1",
    "sass": "^1.77.5",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
